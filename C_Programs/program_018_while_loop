// Purpose	:  To understand how to write a simple program that uses loops

#include <stdio.h>

int main()
{
	int i;

	i = 1;

	while (i <= 10)
	{
		printf("%d", i);
		printf("\n");

		i = i + 1;
	}

	getchar();
	getchar();

	return 0;
}
