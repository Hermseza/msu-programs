// File		:  program_031_i_5i_10i_20i_while
// Goal		:  To complete Lab 31.
// Purpose	:  To use a while loop to display i, 5i, 10i, and 20i.

#include <stdio.h>

int main()
{
	int i;
	i = 1;

	while(i <= 10)
	{
		printf("%5d %5d %5d %5d", i, 5 * i, 10 * i, 20 * i);
		printf("\n");
		i++;
	}

	getchar();
	getchar();

	return 0;
}
