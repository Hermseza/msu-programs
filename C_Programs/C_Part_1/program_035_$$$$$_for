// Purpose	:  To use a for loop to display 5 $s.

#include <stdio.h>

int main()
{
	int i;

	for(i = 1; i <= 10; i++)
	{
		printf("$$$$$\n");
	}

	getchar();
	getchar();

	return 0;
}
