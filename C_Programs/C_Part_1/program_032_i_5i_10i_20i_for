// File		:  program_032_i_5i_10i_20i_for
// Goal		:  To complete Lab 32.
// Purpose	:  To use a for loop to display i, 5i, 10i, and 20i.

#include <stdio.h>

int main()
{
	int i;

	for(i = 1; i <= 10; i++)
	{
		printf("%5d %5d %5d %5d \n", i, 5 * i, 10 * i, 20 * i);
	}

	getchar();
	getchar();

	return 0;
}
