// Purpose	:  To use and understand the new ++ operator.

#include <stdio.h>

int main()
{
	int i;

	for (i = 1; i <= 10; i++)
	{
		printf("%d", i);
		printf("\n");
	}

	getchar();
	getchar();
	
	return 0;
}
