// Goal		:  To complete Lab 19.
// Purpose	:  To implement use of a do while loop.

#include <stdio.h>

int main()
{
	int i;

	i = 1;

	do
	{
		printf("%d", i);
		printf("\n");

		i = i + 1;
	} 
	while (i <= 10);

	getchar();
	getchar();

	return 0;
}
