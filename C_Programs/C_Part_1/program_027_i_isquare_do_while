// Goal		:  To complete Lab 27.
// Purpose	:  To practice using a do while loop.

#include <stdio.h>

int main()
{
	int i;

	i = 1;

	do
	{
		printf("%5d %5d \n", i, i * i);
		i = i + 1;
	}
	while (i <= 10);

	getchar();
	getchar();

	return 0;
	
}
