// Course	:  Csci 180, Section A
// File		:  program_030_i_isquare_icube_do_while
// Goal		:  To complete Lab 30.
// Purpose	:  To use a do while loop to display i, i squared, and i cubed.

#include <stdio.h>

int main()
{
	int i;
	i = 1;

	do
	{
		printf("%5d %5d %5d", i, i * i, i * i * i);
		printf("\n");
		i++;
	}
	while(i <= 10);

	getchar();
	getchar();

	return 0;
}
