// File		:  program_034_$$$$$_while
// Goal		:  To complete Lab 34.
// Purpose	:  To use a while loop to display 5 $ signs per line

#include <stdio.h>

int main()
{
	int i;
	i = 1;

	while(i <= 10)
	{
		printf("$$$$$\n");
		i++;
	}

	getchar();
	getchar();

	return 0;
}
