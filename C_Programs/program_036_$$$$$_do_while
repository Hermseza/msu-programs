// File		:  program_036_$$$$$_do_while
// Goal		:  To complete Lab 36.
// Purpose	:  To use a do while loop to display 5 $s.

#include <stdio.h>

int main()
{
	int i;
	i = 1;

	do
	{
		printf("$$$$$\n");
		i++;
	}
	while(i <= 10);
	
	getchar();
	getchar();
	
	return 0;
}
