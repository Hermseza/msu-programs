// Goal		:  To complete Lab 23.
// Purpose	:  To practice using a for loop.

#include <stdio.h>

int main()
{
	int i;

	for (i = 1; i <= 10; i++)
	{
		printf("%5d %5d \n", i, 10 * i);
	}

	getchar();
	getchar();

	return 0;
	
}
