// Goal		:  To complete Lab 25.
// Purpose	:  To practice using a while loop.

#include <stdio.h>

int main()
{
	int i;

	i = 1;

	while (i <= 10)
	{
		printf("%5d %5d \n", i, i * i);

		i++;
	}

	getchar();
	getchar();

	return 0;
	
}
